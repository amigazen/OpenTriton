; Pack the Triton distribution drawers

failat 21
delete ram:triXXusr.lha ram:triXXdev.lha ram:triXXusr.lzx ram:triXXdev.lzx
arch:lha -x -a -m -r a ram:triXXusr.lha ram:TritonUsr/ Triton Triton.info
arch:lha -x -a -m -r a ram:triXXdev.lha ram:TritonDev/ Triton Triton.info
;arch:lzx -3 -x -a -m -r a ram:triXXusr.lzx ram:TritonUsr/ Triton Triton.info
;arch:lzx -3 -x -a -m -r a ram:triXXdev.lzx ram:TritonDev/ Triton Triton.info
filenote ram:triXXusr.lha "Triton - A GUI system for AmigaOS. Release X.X (usr)"
filenote ram:triXXdev.lha "Triton - A GUI system for AmigaOS. Release X.X (dev)"
;filenote ram:triXXusr.lzx "Triton - A GUI system for AmigaOS. Release X.X (usr)"
;filenote ram:triXXdev.lzx "Triton - A GUI system for AmigaOS. Release X.X (dev)"
